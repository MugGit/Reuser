<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="js/bootstrap.min.js"></script>
<!--FIX NAV TO TOP WHEN SCROLLING-->
<script type="text/javascript">
var max_scroll = 100; // this is the scroll position to start positioning the nav in a fixed way
$(document).ready(function(){

  $(window).scroll(function () {
    var navbar = $(".navbar-static-top");

    var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
    if(scrollTop > max_scroll && !navbar.is(".navbar-static-top-fixed")) {
      navbar.addClass("navbar-static-top-fixed");
                // console.log("go floated");
              }
              else if(scrollTop < max_scroll && navbar.is(".navbar-static-top-fixed") ) {
                // console.log("return to normal");
                navbar.removeClass("navbar-static-top-fixed");
              }

            });
});

</script>

